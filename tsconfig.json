{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@a/*": [
        "./src/assets/*"
      ],
      "@img/*": [
        "./src/assets/images/*"
      ],
      "@l/*": [
        "./src/layouts/*"
      ],
      "@c/*": [
        "./src/components/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}